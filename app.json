{
  "expo": {
    "name": "WildernessSurvivalApp",
    "slug": "WildernessSurvivalApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/grass.png",
    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.thereelrg.WildernessSurvivalApp"
    },
    "android": {
      "softwareKeyboardLayoutMode":"pan",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/grass.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.thereelrg.WildernessSurvivalApp"
    },
    "web": {
      "favicon": "./app/assets/images/favicon.png"
    }
  }
}
